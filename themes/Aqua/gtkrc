   		    file			= "Combo/combo-normal.png"
   		    border			= { 6, 23, 11, 12}
            stretch			= TRUE
        }
        image
        {
            function			= BOX
            recolorable			= TRUE
   		    file			= "Combo/combo-normal.png"
   		    border			= { 6, 23, 11, 12}
            stretch			= TRUE
        }
        image
        {
            function			= TAB
            state				= INSENSITIVE
            recolorable			= TRUE
   		    overlay_file	= "Combo/combo-arrow.png"
   		    overlay_border	= { 0, 0, 0, 0 }
            overlay_stretch		= FALSE
        }
        image
        {
            function			= TAB
            recolorable			= TRUE
   		    overlay_file	= "Combo/combo-arrow.png"
   		    overlay_border	= { 0, 0, 0, 0 }
            overlay_stretch		= FALSE
        }
   }
