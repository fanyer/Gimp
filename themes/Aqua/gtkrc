	    {
			function = ARROW
			recolorable = TRUE
			state = PRELIGHT
		    overlay_file = "Menu-Menubar/menu-arrow-prelight.png"
			overlay_stretch	= FALSE
			arrow_direction	= RIGHT
	    }
 
	}
}

